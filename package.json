{
	"name": "raythis",
	"displayName": "RayThis: Instant Beautiful Code Screenshots",
	"description": "Instantly deploy beautiful code snippets to Ray.so without leaving your coding environment.",
	"icon": "256x256.png",
	"publisher": "Goopware",
	"version": "1.1.0",
	"repository": {
		"url": "https://github.com/ridarf/raythis"
	},
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Snippets",
		"Visualization",
		"Other"
	],
	"keywords": [
		"ray.so",
		"ray",
		"snippets",
		"code snippets"
	],
	"activationEvents": [
		"onCommand:ray-this.publishSelectedSnippet"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ray-this.publishSelectedSnippet",
				"title": "Ray.so"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.53.0",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"open": "^7.4.2",
		"prettier": "^2.3.0"
	}
}
